{
    "name": "Tab Manager",
    "version": "3.14",
    "manifest_version": 2,
    "description": "Lets you very easily and quickly handle your tabs!",
    "icons": {
        "128": "images/icon.png",
        "48": "images/icon48.png"
    },
    "permissions": ["tabs"],
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "images/browser_action.png",
        "default_popup": "popup.html"
    }
}
